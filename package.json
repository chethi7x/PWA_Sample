{
  "name": "app-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/app/translate/translate.en.xlf --output-path=dist/en --baseHref /angular_sample/en/",
    "build-locale:id": "ng build --prod --i18n-locale id --i18n-format xlf --i18n-file src/app/translate/translate.id.xlf --output-path=dist/id --baseHref /angular_sample/id/",
    "build-locale:th": "ng build --prod --i18n-locale th --i18n-format xlf --i18n-file src/app/translate/translate.th.xlf --output-path=dist/th --baseHref /angular_sample/th/",
    "build-locale": "npm run build-locale:id && npm run build-locale:en && npm run build-locale:th"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/forms": "~8.0.3",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/pwa": "^0.803.6",
    "@angular/router": "~8.0.3",
    "@angular/service-worker": "~8.0.3",
    "ng-http-loader": "^6.0.1",
    "ng2-semantic-ui": "^0.9.7",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
